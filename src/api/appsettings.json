{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "{CONNECTION-STRING}",
    "DatabaseName": "{DATABASE-NAME}"
  },
  "Hashing": {
    "HashSizeInBits": "{HASH-SIZE-IN-BITS}",
    "Seed": "{SEED}"
  },
  "JWT": {
    "Key": "{SECRET-KEY}",
    "Issuer": "{ISSUER}",
    "Audience": "{AUDIENCE}"
  }
}

{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "{CONNECTION-STRING}",
    "DatabaseName": "{DATABASE-NAME}"
  },
  "Hashing": {
    "HashSizeInBits": "{HASH-SIZE-IN-BITS}",
    "Seed": "{SEED}"
  },
  "Auth": {
    "Issuer": "{ISSUER}",
    "Audience": "{AUDIENCE}",
    "AccessToken": {
      "SecretKey": "{SECRET-KEY}",
      "ExpirationTimeInMinutes": 5
    },
    "RefreshToken": {
      "SecretKey": "{SECRET-KEY}",
      "ExpirationTimeInMinutes": 43200
    },
    "PasswordRestrictions": {
      "MinimalLength": 8,
      "UppercaseLetters": true,
      "LowercaseLetters": true,
      "Digits": true
    }
  }
}
